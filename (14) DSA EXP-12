#include <stdio.h>

int binarySearch(int arr[], int n, int key) {
    int low = 0, high = n - 1, mid;
    while (low <= high) {
        mid = (low + high) / 2;
        if (arr[mid] == key)
            return mid;
        else if (arr[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }
    return -1;
}

int main() {
    int n, key, result;
    printf("Enter number of products: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter sorted product IDs:\n");
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    printf("Enter product ID to search: ");
    scanf("%d", &key);
    result = binarySearch(arr, n, key);
    if (result == -1)
        printf("Product not found.\n");
    else
        printf("Product found at position %d\n", result + 1);
    return 0;
}
